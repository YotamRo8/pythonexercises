#typefunction
num=10
str="Hello"
bool=True
print (type(num))
print (type(str))
print (type(bool))
print (f"The value of num is {num} and the type is {type(num)}) ")
print (f"The value of str is {str} and the type is {type(str)}) ")
print (f"The value of bool is {bool} and the type is {type(bool)}) ")
