#guessing_games
import random
guessing_number=random.randint(1,10)
user_number=(int (input ("choose a number between 1 to 10")))
#print(guessing_number)
#print (user_number)
while user_number != guessing_number:
    if user_number > guessing_number:
        print ("too high")    
    else:
        print ("too low")
    user_number=(int (input ("try again:")))        
print ("you guessed correct!")
